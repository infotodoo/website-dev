<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="website_requisicions">
        <t t-call="website.layout">
            <t t-set="additional_title">Añadir Requisicion</t>

            <div class="container">
                <h1 class="text-center mt-2">Añadir Requisicion</h1>
                <div class="row mt-3">
                    <section class="col">
                        <form id="add_requisicion" action="/create_requisicion" method="post" class="s_website_form"
                              enctype="multipart/form-data">
                        </form>
                    </section>
                </div>
            </div>
        </t>
    </template>
</odoo>
